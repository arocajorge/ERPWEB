
@{
    ViewBag.Title = "Parroquias";
    if (string.IsNullOrEmpty(Core.Erp.Web.Helps.SessionFixed.IdUsuario) || string.IsNullOrEmpty(Core.Erp.Web.Helps.SessionFixed.IdEmpresa))
    {
        Response.Redirect("~/Account/Login");
    }
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{

    @Html.Action("GridViewPartial_parroquia", new { IdCiudad_canton = ViewBag.IdCiudad_canton, IdProvincia = ViewBag.IdProvincia, IdPais = ViewBag.IdPais })

    <div class="box-footer">
        <a class="btn btn-default" href="@Url.Action("Index","Ciudad",new { IdCiudad_canton = ViewBag.IdCiudad_canton, IdProvincia = ViewBag.IdProvincia, IdPais = ViewBag.IdPais, Area = "General" },null)">Volver</a>
    </div>

}