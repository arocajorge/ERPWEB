@model Core.Erp.Info.Compras.com_catalogo_Info
@{
    ViewBag.Title = "Consulta de catálogos de compra";
    if (string.IsNullOrEmpty(Core.Erp.Web.Helps.SessionFixed.IdUsuario) || string.IsNullOrEmpty(Core.Erp.Web.Helps.SessionFixed.IdEmpresa))
    {
        Response.Redirect("~/Account/Login");
    }
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{
    @Html.HiddenFor(q => q.IdTransaccionSession, new { @id = "IdTransaccionSession" })
    @Html.HiddenFor(q => q.IdCatalogocompra_tipo, new { @id = "IdCatalogocompra_tipo" })

    @Html.Action("GridViewPartial_cat_compras", new { IdCatalogocompra_tipo= Model.IdCatalogocompra_tipo, Nuevo =ViewBag.Nuevo})


    <div class="box-footer">
        <a class="btn btn-default" href="@Url.Action("Index","CatalogoTipoCompras",new { Area = "Compras" },null)">Volver</a>
    </div>

}
