
@{
    ViewBag.Title = "Consulta de contactos de cliente";
    if (string.IsNullOrEmpty(Core.Erp.Web.Helps.SessionFixed.IdUsuario) || string.IsNullOrEmpty(Core.Erp.Web.Helps.SessionFixed.IdEmpresa))
    {
        Response.Redirect("~/Account/Login");
    }
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{
    @Html.Action("GridViewPartial_ClienteContactos", new { IdEmpresa = ViewBag.IdEmpresa, IdCliente = ViewBag.IdCliente })
}


<div class="box-footer">
    <a class="btn btn-default" href="@Url.Action("Index","Cliente",new { Area = "Facturacion" },null)">Volver</a>
</div>