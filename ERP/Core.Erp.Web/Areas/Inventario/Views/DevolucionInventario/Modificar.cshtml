@model Core.Erp.Info.Inventario.in_devolucion_inven_Info
@{
    ViewBag.Title = "Modificar devolución";
    if (Session["IdUsuario"] == null || Session["IdEmpresa"] == null)
    {
        Response.Redirect("~/Account/Login");
    }
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm("Modificar", "DevolucionInventario", FormMethod.Post, new { @class = "form", @role = "form" }))
{
    <div class="box-footer">
        <button type="submit" class="btn btn-primary">Guardar</button>
        <a class="btn btn-default" href="@Url.Action("Index", "DevolucionInventario", null)">Cancelar</a>
    </div>

        @section Scripts {
            @Scripts.Render("~/bundles/jqueryval")
        }
}